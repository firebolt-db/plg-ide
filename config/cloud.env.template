# Firebolt Cloud Configuration
# Copy this to .env and fill in your credentials

FIREBOLT_RUNTIME=cloud

# Your Firebolt Cloud credentials (from Service Account)
FIREBOLT_CLIENT_ID=your_client_id_here
FIREBOLT_CLIENT_SECRET=your_client_secret_here

# Account and database settings
FIREBOLT_ACCOUNT=your_account_name
FIREBOLT_DATABASE=plg_demo
FIREBOLT_ENGINE=plg_demo_engine

# API endpoint (default US East)
FIREBOLT_API_ENDPOINT=api.app.firebolt.io
